<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.shadrach.modelmissionhighschool.Bolarambranch"
    style="@style/androidlayout">


    <ImageView
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/bolaram"/>
    <Button
        style="@style/button"
        android:id="@+id/bolaram_examschedule"
        android:layout_below="@+id/imageView"
        android:layout_marginTop="10dp"
        android:text="EXAM SCHEDULE"
        android:onClick="OnClickBolaramExamSchedule"/>
    <Button
        style="@style/button"
        android:id="@+id/bolaram_timetable"
        android:layout_below="@+id/bolaram_examschedule"
        android:layout_marginTop="10dp"
        android:text="TIME TABLE"
        android:onClick="BolaramTimeTable"/>
</RelativeLayout>
